{
  "id": "org.eyecreate.vehiclevoyage",
  "runtime": "org.kde.Platform",
  "command": "vehiclevoyage",
  "runtime-version": "5.14",
  "sdk": "org.kde.Sdk",
  "finish-args": [
    "--share=ipc",
    "--share=network",
    "--socket=fallback-x11",
    "--socket=wayland",
    "--device=dri",
    "--talk-name=org.freedesktop.Notifications"
  ],
  "separate-locales": false,
  "modules": [
    {
      "name": "vehiclevoyage",
      "buildsystem": "cmake-ninja",
      "builddir": true,
      "config-opts": [
        "-DCMAKE_BUILD_TYPE=Release"
      ],
      "sources": [
        {
          "type": "git",
          "url": "https://codeberg.org/SFaulken/vehicle-voyage.git",
          "commit": "ccfc3f8a47c39b02fbb55ca6b6033b836bb56f09"
        },
        {
          "type": "patch",
          "path": "icon-theme-patch.diff"
        }
      ]
    }
  ]
}
